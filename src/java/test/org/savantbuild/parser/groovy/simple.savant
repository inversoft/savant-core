package org.savantbuild.parser.groovy

project(group: "group", name: "name", version: "1.1")

workflow {
  fetch {
    cache("${SYS['user.home']}/.savant/cache")
    url("http://repository.savantbuild.org")
  }
  publish {
    cache()
  }
}

target(name: "compile", description: "This target compiles everything") {
  project.name = "changed"
}